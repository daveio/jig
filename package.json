    "file-type": "^21.0.0",
    "happy-dom": "^18.0.1",
    "jose": "^6.0.11",
    "js-yaml": "^4.1.0",
    "json5": "^2.2.3",
    "jsonc-parser": "^3.3.1",
    "ms": "^2.1.3",
    "ora": "^8.2.0",
    "readline-sync": "^1.4.10",
    "semver": "^7.7.2",
    "sharp": "^0.34.2",
    "uuid": "^11.1.0"
  },
  "description": "belt CLI",
  "devDependencies": {
    "@anthropic-ai/claude-code": "^1.0.31",
    "@biomejs/biome": "^2.0.2",
    "@openai/codex": "^0.1.2505291658",
    "@trunkio/launcher": "^1.3.4",
    "@types/bun": "^1.2.16",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^24.0.3",
    "@types/readline-sync": "^1.4.8",
    "@types/semver": "^7.7.0",
    "copyfiles": "^2.4.1",
    "npm-run-all2": "^8.0.4",
    "prettier": "^3.5.3",
    "rimraf": "^6.0.1",
    "typescript": "^5.8.3"
  },
  "engines": {
    "bun": "^1.2.16",
    "node": "^22.16.0"
  },
  "license": "MIT",
  "module": "index.ts",
  "name": "belt",
  "packageManager": "bun@1.2.16",
  "prettier": {
    "semi": false,
    "singleQuote": false
  },
  "private": true,
  "scripts": {
    "fmt": "bun run format",
    "format": "bun run-s format:prettier format:biome format:trunk",
    "format:biome": "bun run biome format --write",
    "format:prettier": "bun run prettier --write .",
    "format:trunk": "bun run trunk fmt -a",
    "lint": "bun run-s lint:biome lint:trunk lint:types",
    "lint:biome": "bun run biome check",
    "lint:fix": "bun run-s format lint:fix:biome lint:fix:trunk lint:types",
    "lint:fix:biome": "bun run biome check --write",
    "lint:fix:trunk": "bun run trunk check -a --fix",
    "lint:trunk": "bun run trunk check -a",
    "lint:types": "bun run tsc --noEmit",
    "reset": "bun run rimraf node_modules bun.lock && bun install"
  },
  "type": "module",
  "version": "0.0.1",
  "trustedDependencies": [
    "@anthropic-ai/claude-code",
    "sharp",
    "unrs-resolver"
  ]
}
