{
  "bin": {
    "belt": "bin/belt"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.54.0",
    "@catppuccin/palette": "^1.7.1",
    "@modelcontextprotocol/sdk": "^1.13.0",
    "axios": "^1.10.0",
    "boxen": "^8.0.1",
    "chalk": "^5.4.1",
    "cloudflare": "^4.4.1",
    "date-fns": "^4.1.0",
    "fast-xml-parser": "^5.2.5",
    "file-type": "^21.0.0",
    "gluegun": "^5.2.0",
    "happy-dom": "^18.0.1",
    "jose": "^6.0.11",
    "js-yaml": "^4.1.0",
    "json5": "^2.2.3",
    "jsonc-parser": "^3.3.1",
    "lodash": "^4.17.21",
    "ms": "^2.1.3",
    "ora": "^8.2.0",
    "readline-sync": "^1.4.10",
    "semver": "^7.7.2",
    "sharp": "^0.34.2",
    "uuid": "^11.1.0"
  },
  "resolutions": {
    "axios": "^1.10.0",
    "semver": "^7.7.2",
    "cross-spawn": "^7.0.6",
    "lodash.trim": "npm:lodash@^4.17.21",
    "lodash.trimend": "npm:lodash@^4.17.21",
    "ejs": "^3.1.10"
  },
  "description": "belt CLI",
  "devDependencies": {
    "@anthropic-ai/claude-code": "^1.0.31",
    "@biomejs/biome": "^2.0.2",
    "@openai/codex": "^0.1.2505291658",
    "@trunkio/launcher": "^1.3.4",
    "@types/bun": "^1.2.16",
    "@types/jest": "^30.0.0",
    "@types/js-yaml": "^4.0.9",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^24.0.3",
    "@types/readline-sync": "^1.4.8",
    "@types/semver": "^7.7.0",
    "copyfiles": "^2.4.1",
    "husky": "^9.1.7",
    "jest": "^30.0.2",
    "npm-run-all2": "^8.0.4",
    "prettier": "^3.5.3",
    "pretty-quick": "^4.2.2",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.4.0",
    "ts-node": "10.9.2",
    "typescript": "^5.8.3"
  },
  "engines": {
    "bun": "^1.2.16",
    "node": "^22.16.0"
  },
  "files": [
    "build",
    "LICENSE",
    "readme.md",
    "docs",
    "bin"
  ],
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node"
  },
  "license": "MIT",
  "module": "src/cli.ts",
  "name": "belt",
  "packageManager": "bun@1.2.16",
  "prettier": {
    "semi": false,
    "singleQuote": false
  },
  "private": true,
  "scripts": {
    "build": "bun run-s reset build:tsc build:templates",
    "build:templates": "bun run copyfiles ./src/templates/* ./build/templates",
    "build:tsc": "tsc -p .",
    "fmt": "bun run format",
    "format": "bun run-s format:prettier format:biome format:trunk",
    "format:biome": "bun run biome format --write",
    "format:prettier": "bun run prettier --write .",
    "format:trunk": "bun run trunk fmt -a",
    "lint": "bun run-s lint:biome lint:trunk lint:types",
    "lint:biome": "bun run biome check",
    "lint:fix": "bun run-s format lint:fix:biome lint:fix:trunk lint:types",
    "lint:fix:biome": "bun run biome check --write",
    "lint:fix:trunk": "bun run trunk check -a --fix",
    "lint:trunk": "bun run trunk check -a",
    "lint:types": "bun run tsc --noEmit",
    "prepublishOnly": "bun run build",
    "reset": "bun run rimraf node_modules bun.lock build && bun install",
    "start": "bun src/cli.ts",
    "test": "bun run-s test:jest test:coverage",
    "test:coverage": "bun run jest --coverage",
    "test:jest": "bun run jest",
    "test:jest:snapshot": "jest --updateSnapshot",
    "test:jest:watch": "jest --watch"
  },
  "type": "module",
  "types": "build/types/types.d.ts",
  "version": "0.0.1",
  "trustedDependencies": [
    "@anthropic-ai/claude-code",
    "sharp",
    "unrs-resolver"
  ]
}
